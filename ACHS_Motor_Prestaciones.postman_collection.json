		{
			"name": "DDT_Calculo_Beneficio",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const json = pm.response.json();\r",
							"const data = pm.iterationData.toObject();\r",
							"\r",
							"pm.test(\"Status debe ser 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Tipo de beneficio correcto\", () => {\r",
							"    pm.expect(json.tipoBeneficio).to.eql(data.expectedTipo);\r",
							"});\r",
							"\r",
							"pm.test(\"Monto correcto\", () => {\r",
							"    pm.expect(json.monto).to.eql(Number(data.expectedMonto));\r",
							"});\r",
							"\r",
							"if (data.expectedPeriodicidad) {\r",
							"    pm.test(\"Periodicidad correcta\", () => {\r",
